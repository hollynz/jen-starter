            {
                "id": "5d12909a90e07d077d918e5c",
                "slug": "easily-customizable-p7ifr7h77-",
                "parents": [],
                "tags": [],
                "createdAt": "2019-06-25T21:22:34.309Z",
                "updatedAt": "2019-06-25T21:22:34.309Z",
                "author": {
                    "_id": "5d12903690e07d077d918e51",
